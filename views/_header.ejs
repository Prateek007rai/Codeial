<header>
    <h1 >Codeial</h1>

      <ul>
        <% if(locals.user){ %>
            <li><a href="/users/profile/<%= user.id %>">
                <%= user.name %> </a>
            </li>
            <li>
                <a href="/users/Sign-Out">Sign Out</a>
            </li>

        <% } else { %>
            <li>
                <a href="/users/Sign-In">Sign In</a>
            </li>
            <li>
                <a href="/users/Sign-Up">Sign Up</a>
            </li>
                 
        <% } %>
      </ul>

</header>